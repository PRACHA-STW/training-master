<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>FSM_JobSLAInHours__c</fullName>
    <description>Created as a part of SFS-4427</description>
    <externalId>false</externalId>
    <formula>IF(FSM_JobSLAUnitOfMeasure__c = &apos;Minutes&apos; , (FSM_JobSLA__c / 60) ,
IF(FSM_JobSLAUnitOfMeasure__c = &apos;Hours&apos;, FSM_JobSLA__c, 0))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Job SLA In Hours</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
