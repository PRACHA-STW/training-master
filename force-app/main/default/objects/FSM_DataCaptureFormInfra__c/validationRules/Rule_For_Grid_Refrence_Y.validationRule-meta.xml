<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Rule_For_Grid_Refrence_Y</fullName>
    <active>true</active>
    <description>Created as a part of SFS-4087</description>
    <errorConditionFormula>OR(  FSM_GridsReferenceY__c  &lt; 170000, FSM_GridsReferenceY__c &gt; 440000)</errorConditionFormula>
    <errorDisplayField>FSM_GridsReferenceY__c</errorDisplayField>
    <errorMessage>The Value should be within 170000 and 440000</errorMessage>
</ValidationRule>
