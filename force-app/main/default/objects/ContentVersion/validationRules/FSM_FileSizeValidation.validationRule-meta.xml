<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>FSM_FileSizeValidation</fullName>
    <active>false</active>
    <errorConditionFormula>AND( NOT($Permission.FSM_Integration),ContentSize &gt; 6300000)</errorConditionFormula>
    <errorDisplayField>ContentSize</errorDisplayField>
    <errorMessage>File Size exceeds 6 MB</errorMessage>
</ValidationRule>
