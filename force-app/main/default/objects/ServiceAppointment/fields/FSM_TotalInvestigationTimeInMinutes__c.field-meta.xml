<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>FSM_TotalInvestigationTimeInMinutes__c</fullName>
    <externalId>false</externalId>
    <formula>IF( NOT(ISNULL(FSM_InvestigationStartTime__c))&amp;&amp; 
 NOT(ISNULL(FSM_InvestigationEndTime__c)) , (FSM_InvestigationEndTime__c - FSM_InvestigationStartTime__c) * 24 * 60, 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Cumulative Investigation Time (Minutes)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
