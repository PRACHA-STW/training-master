<?xml version="1.0" encoding="UTF-8"?>
<ListView xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>FSM_PlannedJobsNI</fullName>
    <booleanFilter>1 AND 2 AND 3 AND 4</booleanFilter>
    <columns>FSM_IconForNonInfra__c</columns>
    <columns>Status</columns>
    <columns>FSM_AllocatedJob__c</columns>
    <filterScope>Everything</filterScope>
    <filters>
        <field>SchedStartTime</field>
        <operation>greaterOrEqual</operation>
        <value>LAST_N_DAYS:7</value>
    </filters>
    <filters>
        <field>SchedStartTime</field>
        <operation>lessOrEqual</operation>
        <value>NEXT_N_DAYS:7</value>
    </filters>
    <filters>
        <field>FSM_AssignedtoLoginUser__c</field>
        <operation>equals</operation>
        <value>1</value>
    </filters>
    <filters>
        <field>Status</field>
        <operation>equals</operation>
        <value>Dispatched,En-Route,Onsite,On Hold,Return to Site</value>
    </filters>
    <label>NI - Planned Jobs</label>
    <sharedTo>
        <group>FSM_FW_BioResource</group>
        <group>FSM_FW_WasteNonInfra</group>
        <group>FSM_FW_WaterNonInfra</group>
    </sharedTo>
</ListView>
