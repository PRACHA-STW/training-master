<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>FSM_TimeDuration__c</fullName>
    <description>Created as a part of SFI-1557</description>
    <externalId>false</externalId>
    <formula>IF(NOT((ISNULL(FSM_AdjustedTimeDuration__c )||FSM_AdjustedTimeDuration__c==0)) ,FSM_AdjustedTimeDuration__c, FLOOR((EndTime-StartTime) * 24 * 60))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Time Duration</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
