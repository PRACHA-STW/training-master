    <decisions>
        <description>SFS-231

Added FSM Site Worker outcome as part of SFL-1284 //Anup</description>
        <name>check_User_profile</name>
        <label>check User profile</label>
        <locationX>786</locationX>
        <locationY>1192</locationY>
        <defaultConnector>
            <targetReference>Get_Admin_Group</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Other profile</defaultConnectorLabel>
        <rules>
            <name>System_Admin_profile</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Profile.Name</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>FSM System Administrator</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Profile.Name</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>System Administrator</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Copy_1_of_Get_Admin_Group</targetReference>
            </connector>
            <label>System Admin profile</label>
        </rules>
        <rules>
            <name>FSM_Admin_profile</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Profile.Name</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>FSM Field Service Admin</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Copy_1_of_Get_FSM_Admin_Group</targetReference>
            </connector>
            <label>FSM Admin profile</label>
        </rules>
        <rules>
            <name>FSM_Site_Worker</name>
            <conditionLogic>(1 AND 3) OR (1 AND 4 AND 2)</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Profile.Name</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>FSM Site Worker</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record__Prior.Profile.Name</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>FSM Site Worker</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>ISNEW</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>ISNEW</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_FSM_Workmate_Group</targetReference>
            </connector>
            <label>FSM Site Worker</label>
        </rules>
