    <formulas>
        <description>Created as part of SFS-1430</description>
        <name>CumulativeNonProductiveTimeFormula</name>
        <dataType>Number</dataType>
        <expression>{!LatestNonProductiveTimeFormula} + IF(ISBLANK({!SASingleRecordVar.FSM_CumulativeNonProductiveTime__c}), 0, {!SASingleRecordVar.FSM_CumulativeNonProductiveTime__c})</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <description>SFS-3049</description>
        <name>CumulativeProductiveTimeFormulaBoth</name>
        <dataType>Number</dataType>
        <expression>{!LatestProductiveTimeBoth}+{!SASingleRecordVar.FSM_CumulativeProductiveTimeInMins__c}</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <description>SFS-3049</description>
        <name>CumulativeProductiveTimeFormulaEnd</name>
        <dataType>Number</dataType>
        <expression>IF(ISBLANK({!LatestProductiveTimeEnd}), 0, {!LatestProductiveTimeEnd})+{!SASingleRecordVar.FSM_CumulativeProductiveTimeInMins__c}</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <description>SFS-3049</description>
        <name>CumulativeProductiveTimeFormulaStart</name>
        <dataType>Number</dataType>
        <expression>{!LatestProductiveTimeFormulaStart}+{!SASingleRecordVar.FSM_CumulativeProductiveTimeInMins__c}</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <description>Created as part of SFS-1430</description>
        <name>LatestNonProductiveTimeFormula</name>
        <dataType>Number</dataType>
        <expression>({!Enter_Non_Productive_End_Time} - {!SASingleRecordVar.FSM_NonProductiveStartTime__c})*24*60</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <description>SFS-3049</description>
        <name>LatestProductiveTimeBoth</name>
        <dataType>Number</dataType>
        <expression>({!ProductiveEndTime}-{!SASingleRecordVar.FSM_ProductiveStartTime__c})*24*60</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <description>For SFS-3049</description>
        <name>LatestProductiveTimeEnd</name>
        <dataType>Number</dataType>
        <expression>({!ProductiveEndTime}-{!ProductiveStartTime})*24*60</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <description>SFS-3049</description>
        <name>LatestProductiveTimeFormulaStart</name>
        <dataType>Number</dataType>
        <expression>({!ProductiveEndTime}-{!SASingleRecordVar.FSM_ProductiveStartTime__c})*24*60</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <description>Created as part of SFS-1430</description>
        <name>NonProductiveEndTimeNullFormula</name>
        <dataType>DateTime</dataType>
        <expression>NULL</expression>
    </formulas>
