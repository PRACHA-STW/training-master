    <decisions>
        <description>SFS-3049</description>
        <name>Check_If_SA_is_in_Onsite_and_Productive_Start_Time_Present</name>
        <label>Check If SA is in Onsite and Productive Start Time Present</label>
        <locationX>1678</locationX>
        <locationY>566</locationY>
        <defaultConnector>
            <targetReference>Set_Latest_Non_Productive_Time_and_Productive_Time_Both</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Not Present</defaultConnectorLabel>
        <rules>
            <name>Yes_Present</name>
            <conditionLogic>(1 OR 3) AND 2</conditionLogic>
            <conditions>
                <leftValueReference>SASingleRecordVar.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Onsite</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>SASingleRecordVar.FSM_ProductiveStartTime__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>SASingleRecordVar.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Return to Site</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_Productive_End_Time_Both</targetReference>
            </connector>
            <label>Yes Present</label>
        </rules>
    </decisions>
