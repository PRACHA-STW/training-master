        <assignNextValueToReference>varOpenGroup</assignNextValueToReference>
        <collectionReference>OpenFormsGroup</collectionReference>
        <iterationOrder>Asc</iterationOrder>
        <nextValueConnector>
            <targetReference>Has_an_Entry_in_Completed_Group</targetReference>
        </nextValueConnector>
    </loops>
